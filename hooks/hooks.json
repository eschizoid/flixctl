    "execute-command": "/opt/webhook-linux-amd64/torrent.sh",
    "command-working-directory": "/home/webhook/go/bin",
    "response-message": "Executing search command",
    "pass-environment-to-command": [
      {
        "source": "payload",
        "name": "notify",
        "envname": "SLACK_NOTIFICATION"
      },
      {
        "source": "payload",
        "name": "text",
        "envname": "KEYWORDS"
      },
      {
        "source": "payload",
